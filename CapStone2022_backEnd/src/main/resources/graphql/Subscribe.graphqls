extend type Query {
    getSubscribeCount(fromMemberId : ID!) : Int!
}

extend type Mutation {
    addSubscribe(fromMemberId : ID!, toMemberId : ID!) : Subscribe
    deleteSubscribe(fromMemberId : ID!, toMemberId : ID!) : Subscribe
}

type Subscribe {
    id : ID!
    fromMember : Member
    toMember : Member
}