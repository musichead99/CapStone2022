extend type Query {
    getSubscriberCount(toMemberId : ID!) : Int!
}

extend type Mutation {
    addSubscribe(fromMemberId : ID!, toMemberId : ID!) : SubscribeOutput
    deleteSubscribe(fromMemberId : ID!, toMemberId : ID!) : SubscribeOutput
}

type Subscribe {
    id : ID!
    fromMember : Member!
    toMember : Member!
}

type SubscribeOutput {
    fromMember : Member!
    toMember : Member!
    subscriberCount : Int!
}