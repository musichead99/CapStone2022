extend type Query {
    getMember(id : ID!) : Member
    members : [Member!]
    getSubscriberList(id : ID!) : [Member!]
}

extend type Mutation {
    addMember(memberDto : MemberInput) : Member
    updateMember(id : ID!, memberDto : MemberInput) : Member
}

type Member {
    id : ID!
    email : String!
    name : String!
    picture : String
    posts : [Post]
    comments : [Comment!]
}

input MemberInput {
    email : String!
    article : String
    name : String!
}