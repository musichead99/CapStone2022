scalar LocalDateTime

type Query {
    posts : [Post]
    getPost(id : ID!) : Post
}

type Mutation {
    addPost(postDto : postInput) : Post
    deletePost(id : ID!) : Post
}

type Post {
    id : ID!
    title : String!
    content : String!
    member : Member!
    board : Board!
    comments : [Comment]
    createdAt : LocalDateTime!
}

input postInput {
    title : String!
    content : String!
    memberId : Int!
    boardId : Int!
}